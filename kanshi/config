profile laptop_with_monitor {
        output "Samsung Display Corp. 0x4180 Unknown" disable
        output "LG Electronics LG HDR 4K 0x00066A59" {
                mode 3840x2160
                position 0,0
                scale 1.3
        }
}

profile dell_work_monitor {
        output "Dell Inc. DELL P3223QE 6QD0XN3" {
                mode 3840x2160
                position 0,0
                scale 1.3
        }

        output "Samsung Display Corp. 0x4180 Unknown" disable
}

