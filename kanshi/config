        output "Dell Inc. DELL P3223QE 6QD0XN3" {
                mode 3840x2160
                position 0,0
                scale 1.3
        }

        output "eDP-1" disable
}

profile laptop_screen_only {
        output "Samsung Display Corp. 0x4180 Unknown" enable
}

